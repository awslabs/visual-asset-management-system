{
    "name": "infra",
    "version": "0.1.0",
    "license": "Apache-2.0",
    "bin": {
        "infra": "bin/infra.js"
    },
    "scripts": {
        "build": "tsc",
        "watch": "tsc -w",
        "test": "jest",
        "cdk": "cdk",
        "cdk.synth.app": "cdk synth --all --require-approval never",
        "cdk.deploy.app": "cdk deploy --all --require-approval never",
        "deploy.dev": "npm run cdk.deploy.app -- --context ",
        "deploy.vams": "cdk deploy --all --require-approval never -c stack-name=vams -c region=us-east-1 --context",
        "gen": "ts-node ./gen/genEndpoints.ts"
    },
    "devDependencies": {
        "@aws-cdk/assert": "^2.68.0",
        "@types/jest": "^30.0.0",
        "@types/node": "^24.3.1",
        "@types/prettier": "^3.0.0",
        "aws-cdk": "^2.1029.2",
        "esbuild": "^0.25.10",
        "jest": "^30.1.3",
        "ts-jest": "^29.4.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.2"
    },
    "dependencies": {
        "@aws-cdk/aws-lambda-python-alpha": "^2.214.0-alpha.0",
        "@aws-sdk/client-iam": "^3.883.0",
        "@aws-sdk/client-ssm": "^3.883.0",
        "@opensearch-project/opensearch": "^3.5.1",
        "@types/aws-lambda": "^8.10.152",
        "@types/aws4": "^1.11.6",
        "aws-cdk-lib": "^2.215.0",
        "aws4": "^1.13.2",
        "cdk-nag": "^2.37.29",
        "constructs": "^10.4.2",
        "dotenv": "^17.2.2",
        "source-map-support": "^0.5.21"
    }
