{
    "name": "infra",
    "version": "0.1.0",
    "license": "Apache-2.0",
    "bin": {
        "infra": "bin/infra.js"
    },
    "scripts": {
        "build": "tsc",
        "watch": "tsc -w",
        "test": "jest",
        "cdk": "cdk",
        "cdk.synth.app": "cdk synth",
        "cdk.deploy.app": "cdk deploy --all",
        "deploy.dev": "npm run cdk.deploy.app -- --require-approval never --context ",
        "deploy.dev.pipeline": "npm run cdk.deploy.app -- --require-approval never --context pipelineActivatePCVisualizer=true --context ",
        "deploy.vams": "cdk deploy --all -c stack-name=vams -c region=us-east-1 --require-approval never --context",
        "deploy.vams.pipeline": "cdk deploy --all -c stack-name=vams -c region=us-east-1 --require-approval never --context pipelineActivatePCVisualizer=true --context"
    },
    "devDependencies": {
        "@aws-cdk/assert": "^2.68.0",
        "@types/jest": "^29.5.0",
        "@types/node": "^18.16.17",
        "@types/prettier": "^2.7.2",
        "aws-cdk": "^2.86.0",
        "jest": "^29.5.0",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "typescript": "~4.8.4"
    },
    "dependencies": {
        "@aws-cdk/aws-apigatewayv2-alpha": "^2.47.0-alpha.0",
        "@aws-cdk/aws-apigatewayv2-authorizers-alpha": "^2.47.0-alpha.0",
        "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.47.0-alpha.0",
        "@aws-cdk/aws-batch-alpha": "^2.70.0-alpha.0",
        "@aws-cdk/aws-lambda-python-alpha": "^2.47.0-alpha.0",
        "@aws-sdk/client-opensearchserverless": "^3.421.0",
        "@aws-sdk/client-ssm": "^3.421.0",
        "@opensearch-project/opensearch": "^1.2.0",
        "@types/aws-lambda": "^8.10.117",
        "@types/aws4": "^1.11.3",
        "aws-cdk-lib": "^2.99.1",
        "aws-lambda": "^1.0.7",
        "aws-sdk": "^2.1467.0",
        "aws4": "^1.12.0",
        "cdk-nag": "^2.25.144",
        "constructs": "^10.2.70",
        "npm-check-updates": "^16.10.7",
        "source-map-support": "^0.5.21"
    }
}
